CUDA_VISIBLE_DEVICES=3 python train_voc.py --net='fpn50' --tag=fpn50 --iters=70000 --cfg='./experiments/cfgs/fpn50.yml' --weight='./data/pretrained_model/resnet50_caffe.pth'
CUDA_VISIBLE_DEVICES=4 python test.py --net='fpn50' --tag=fpn50 --cfg='./experiments/cfgs/fpn50.yml' --model=70000 --model_path='voc_2007_trainval/fpn50' --imdb='voc_2007_test' --comp